   "source": [
    "def sumOfArray(arr):\n",
    "    result = 0\n",
    "\n",
    "    for num in arr:\n",
    "        result = result + num\n",
    "    \n",
    "    return result\n",
    "\n",
    "def averageOfAllElements(arr):\n",
    "    return sumOfArray(arr) / len(arr)\n",
    "\n",
    "arr = [10,20,30]\n",
    "print(averageOfAllElements(arr))"
   ]
  }
